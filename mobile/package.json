    "expo": "~53.0.17",
    "expo-camera": "^16.1.10",
    "expo-image-picker": "^16.1.4",
    "expo-location": "^18.1.6",
    "expo-status-bar": "~2.2.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "^2.27.1",
